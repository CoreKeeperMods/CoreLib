            <ui:VisualElement name="Icon" class="hierarchy-item__icon"/>
            <ue:HierarchyNameElement name="Name" class="hierarchy-item__name"/>
            <ui:Label name="SubSceneState" class="hierarchy-item__subscene-state-label"/>
