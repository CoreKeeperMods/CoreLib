.build-component__header
{
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    background-color: transparent;
    padding: 3px;
    border-width: 1px 0px;
    border-radius: 0px;
    border-color: transparent;
    margin: 0px;
}

.build-component__header-left
{
    flex-shrink: 0;
    align-items: center;
    flex-direction: row;
}

.build-component__header-right
{
    flex-shrink: 1;
    align-items: center;
    flex-direction: row;
}

.build-component__foldout
{
    flex-shrink: 0;
    background-color: transparent;
    width: 13px;
    height: 13px;
    padding: 0px;
    border-width: 0px;
    border-radius: 0px;
    border-color: transparent;
    margin: 0px 0px 0px 2px;
}

